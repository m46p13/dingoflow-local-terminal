DINGOFLOW_ASR_BACKEND=parakeet-native
DINGOFLOW_RECORDER_BACKEND=auto
# If recorder backend is native-rust or auto:
DINGOFLOW_NATIVE_AUDIO_BIN=/absolute/path/to/dingoflow/native/audio_loop/target/release/dingoflow-audio-loop
# Optional native text injector (faster than osascript AX path):
DINGOFLOW_NATIVE_TEXT_INJECT_BIN=/absolute/path/to/dingoflow/native/text_injector/bin/dingoflow-text-injector
# If ASR backend is whisper-native:
DINGOFLOW_NATIVE_ASR_BIN=/absolute/path/to/dingoflow/native/asr_worker/target/release/dingoflow-asr-worker
# If ASR backend is parakeet-native:
DINGOFLOW_NATIVE_PARAKEET_BIN=/absolute/path/to/dingoflow/native/parakeet_worker/target/release/dingoflow-parakeet-worker
DINGOFLOW_NATIVE_ASR_THREADS=4
DINGOFLOW_HOTKEY=CommandOrControl+Shift+Space
DINGOFLOW_FFMPEG_INPUT=:0
DINGOFLOW_PYTHON_BIN=/absolute/path/to/.venv/bin/python
DINGOFLOW_ASR_MODEL_PATH=/absolute/path/to/models/parakeet-tdt-0.6b-v3-onnx
# If using faster-whisper backend:
# DINGOFLOW_ASR_MODEL_PATH=/absolute/path/to/models/faster-whisper-small.en
# If using parakeet-mlx backend:
# DINGOFLOW_ASR_MODEL_PATH=/absolute/path/to/models/parakeet-tdt-0.6b-v3
# If using parakeet-native backend:
# DINGOFLOW_ASR_MODEL_PATH=/absolute/path/to/models/parakeet-tdt-0.6b-v3-onnx
# If using whisper-native backend:
# DINGOFLOW_ASR_MODEL_PATH=/absolute/path/to/models/ggml-base.en.bin
DINGOFLOW_FORMATTER_MODEL_PATH=/absolute/path/to/models/qwen2.5-0.5b-instruct-mlx-4bit
DINGOFLOW_ASR_DEVICE=cpu
DINGOFLOW_ASR_COMPUTE_TYPE=int8
DINGOFLOW_ASR_TRANSPORT=framed
DINGOFLOW_FORMATTER_MAX_TOKENS=240
DINGOFLOW_PASTE_DELAY_MS=80
DINGOFLOW_INJECTION_RETRY_COUNT=2
DINGOFLOW_INJECTION_RETRY_DELAY_MS=120
DINGOFLOW_SPOKEN_FORMATTING_COMMANDS=true

# Latency profile: ultra | balanced | quality
DINGOFLOW_LATENCY_PRESET=balanced

# Low-latency streaming tuning
DINGOFLOW_STREAM_CHUNK_MS=90
DINGOFLOW_ASR_WINDOW_MIN_MS=70
DINGOFLOW_ASR_WINDOW_NORMAL_MS=140
DINGOFLOW_ASR_WINDOW_BACKLOG_MS=300
DINGOFLOW_ASR_WINDOW_MAX_MS=520
DINGOFLOW_ASR_WINDOW_ADAPTIVE=true
# Latency-first default (set true to run a full final correction pass on key release)
DINGOFLOW_PARAKEET_FINAL_PASS=false
# Silence suppression (reduces hallucinations on background noise)
DINGOFLOW_SILENCE_GATE_DBFS=-52
DINGOFLOW_SPEECH_HANGOVER_MS=420

# Parakeet stateful streaming tuning
DINGOFLOW_PARAKEET_CTX_LEFT=64
DINGOFLOW_PARAKEET_CTX_RIGHT=8
DINGOFLOW_PARAKEET_STREAM_DEPTH=1

DINGOFLOW_ENFORCE_OFFLINE=true
